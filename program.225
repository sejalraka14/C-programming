#include <stdio.h>
int main(){
    int s[5], sum=0;
    printf("Enter 5 readings: ");
    for(int i=0;i<5;i++){ scanf("%d",&s[i]); sum+=s[i]; }
    float avg = (float)sum/5;
    printf("Average = %.2f\n", avg);
    if(avg > 50) printf("Sensor Performance: GOOD");
    else printf("Sensor Performance: WEAK");
    return 0;
}
