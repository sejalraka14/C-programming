#include <stdio.h>
float avg(int arr[3][3]){
    int sum=0;
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
            sum+=arr[i][j];
    return (float)sum/9;
}
int main(){
    int data[3][3];
    printf("Enter 3x3 sensor data (3 days x 3 sensors):\n");
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
            scanf("%d",&data[i][j]);
    printf("Overall Average = %.2f", avg(data));
    return 0;
}
