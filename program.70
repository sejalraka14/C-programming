#include<stdio.h>
#include<conio.h>
void main()
{
    float marks[5], sum=0, avg;
    int i;
    clrscr();
    printf("Enter marks of 5 subjects:\n");
    for(i=0; i<5; i++)
    {
        scanf("%f", &marks[i]);
        sum += marks[i];
    }
    avg = sum / 5;
    printf("Average marks = %.2f", avg);
    getch();
}
