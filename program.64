#include<stdio.h>
#include<conio.h>
void main()
{
    int a[10][10], b[10][10];
    float c[10][10];
    int i, j, r, col;
    clrscr();
    printf("Enter rows and cols: ");
    scanf("%d%d", &r, &col);

    printf("Enter elements of first matrix:\n");
    for(i=0; i<r; i++)
        for(j=0; j<col; j++)
            scanf("%d", &a[i][j]);

    printf("Enter elements of second matrix:\n");
    for(i=0; i<r; i++)
        for(j=0; j<col; j++)
            scanf("%d", &b[i][j]);

    for(i=0; i<r; i++)
        for(j=0; j<col; j++)
            if(b[i][j] != 0)
                c[i][j] = (float)a[i][j] / b[i][j];
            else
                c[i][j] = 0; // handle division by zero

    printf("Resultant Matrix (Division):\n");
    for(i=0; i<r; i++)
    {
        for(j=0; j<col; j++)
            printf("%.2f ", c[i][j]);
        printf("\n");
    }
    getch();
}
