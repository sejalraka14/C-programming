#include<stdio.h>
#include<conio.h>
void main()
{
    int a[100], n, i, diff;
    clrscr();
    printf("Enter size of array: ");
    scanf("%d", &n);
    printf("Enter %d elements:\n", n);
    for(i=0; i<n; i++)
        scanf("%d", &a[i]);
    
    diff = a[0];
    for(i=1; i<n; i++)
        diff -= a[i];
    
    printf("Subtraction of array elements = %d", diff);
    getch();
}
