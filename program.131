#include<stdio.h>
#include<conio.h>
void main()
{
    int marks[5],total=0,i;
    float avg,percent;
    clrscr();
    printf("Enter marks of 5 subjects:\n");
    for(i=0;i<5;i++)
    {
        scanf("%d",&marks[i]);
        total+=marks[i];
    }
    avg=total/5.0;
    percent=(total/500.0)*100;
    printf("Total=%d, Average=%.2f, Percentage=%.2f%%",total,avg,percent);
    getch();
}
