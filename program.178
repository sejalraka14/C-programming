#include <stdio.h>
float avg(int arr[], int n) {
    int sum = 0;
    for(int i = 0; i < n; i++)
        sum += arr[i];
    return (float)sum / n;
}
int main() {
    int data[10];
    printf("Enter 10 sensor readings:\n");
    for(int i = 0; i < 10; i++) scanf("%d", &data[i]);
    printf("Average Value = %.2f", avg(data, 10));
    return 0;
}
