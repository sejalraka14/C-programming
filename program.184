#include <stdio.h>
float trimmedAvg(int a[],int n){
    int sum=0,max=a[0],min=a[0];
    for(int i=0;i<n;i++){
        if(a[i]>max) max=a[i];
        if(a[i]<min) min=a[i];
        sum+=a[i];
    }
    return (float)(sum-max-min)/(n-2);
}
int main(){
    int r[5];
    printf("Enter 5 readings: ");
    for(int i=0;i<5;i++) scanf("%d",&r[i]);
    printf("Trimmed Average = %.2f", trimmedAvg(r,5));
    return 0;
}
