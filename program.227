#include <stdio.h>
void sort(int a[], int n){
    for(int i=0;i<n-1;i++)
        for(int j=i+1;j<n;j++)
            if(a[i]>a[j]){ int t=a[i]; a[i]=a[j]; a[j]=t; }
}
int main(){
    int s[5];
    printf("Enter 5 readings: ");
    for(int i=0;i<5;i++) scanf("%d",&s[i]);
    sort(s,5);
    float avg = (s[2]+s[3]+s[4])/3.0;
    printf("Average of top 3 readings = %.2f", avg);
    return 0;
}
