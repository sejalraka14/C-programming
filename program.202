#include <stdio.h>
float avgValid(int a[], int n){
    int sum=0,c=0;
    for(int i=0;i<n;i++){
        if(a[i]!=-1){ sum+=a[i]; c++; }
    }
    return c? (float)sum/c : 0;
}
int main(){
    int s[5];
    printf("Enter 5 readings (-1 if error): ");
    for(int i=0;i<5;i++) scanf("%d",&s[i]);
    printf("Valid Average = %.2f", avgValid(s,5));
    return 0;
}
