#include<stdio.h>
#include<conio.h>
void main()
{
    int marks[5], i, sum=0;
    float avg;
    clrscr();
    printf("Enter marks of 5 subjects:\n");
    for(i=0; i<5; i++)
    {
        scanf("%d", &marks[i]);
        sum += marks[i];
    }
    avg = sum / 5.0;
    printf("Average marks = %.2f", avg);
    getch();
}
