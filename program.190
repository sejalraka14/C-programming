#include <stdio.h>
#include <math.h>
float avg(int a[],int n){
    int s=0;
    for(int i=0;i<n;i++) s+=a[i];
    return (float)s/n;
}
float stddev(int a[],int n){
    float mean=avg(a,n),sum=0;
    for(int i=0;i<n;i++)
        sum+=pow(a[i]-mean,2);
    return sqrt(sum/n);
}
int main(){
    int r[5];
    printf("Enter 5 readings: ");
    for(int i=0;i<5;i++) scanf("%d",&r[i]);
    printf("Average = %.2f, SD = %.2f", avg(r,5), stddev(r,5));
    return 0;
}
