#include <stdio.h>
#include <conio.h>
void main() {
    int c;
    clrscr();
    printf("1.Forward\n2.Backward\n3.Left\n4.Right\nEnter: ");
    scanf("%d", &c);
    switch(c) {
        case 1: printf("Moving Forward"); break;
        case 2: printf("Moving Backward"); break;
        case 3: printf("Turning Left"); break;
        case 4: printf("Turning Right"); break;
        default: printf("Invalid Command");
    }
    getch();
}
