#include <stdio.h>
#include <stdlib.h>
#include <time.h>
int main(){
    int a[5], sum=0;
    srand(time(0));
    printf("Random readings: ");
    for(int i=0;i<5;i++){
        a[i] = rand()%100;
        printf("%d ", a[i]);
        sum += a[i];
    }
    printf("\nAverage = %.2f", (float)sum/5);
    return 0;
}
