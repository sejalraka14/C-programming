#include <stdio.h>
#include <conio.h>
void main() {
    int cmd;
    clrscr();
    printf("1.Forward\n2.Backward\n3.Left\n4.Right\n5.Stop\nEnter command: ");
    scanf("%d",&cmd);
    switch(cmd){
        case 1: printf("Robot moving Forward."); break;
        case 2: printf("Robot moving Backward."); break;
        case 3: printf("Turning Left."); break;
        case 4: printf("Turning Right."); break;
        case 5: printf("Robot Stopped."); break;
        default: printf("Invalid Command!");
    }
    getch();
}
