#include<stdio.h>
#include<conio.h>
void main()
{
    int n, div=1, digit;
    clrscr();
    printf("Enter a number: ");
    scanf("%d", &n);
    while(n > 0)
    {
        digit = n % 10;
        if(digit != 0)
            div /= (float)digit;
        n = n / 10;
    }
    printf("Division of digits = %.2f", (float)div);
    getch();
}
