#include <stdio.h>
struct Sensor {
    int id;
    int value;
};
float avg(struct Sensor s[], int n){
    int sum=0;
    for(int i=0;i<n;i++) sum+=s[i].value;
    return (float)sum/n;
}
int main(){
    struct Sensor s[3];
    printf("Enter ID and reading for 3 sensors:\n");
    for(int i=0;i<3;i++)
        scanf("%d %d", &s[i].id, &s[i].value);
    printf("Average Reading = %.2f", avg(s,3));
    return 0;
}
