#include <stdio.h>
float average(int arr[], int n) {
    int sum = 0;
    for(int i = 0; i < n; i++)
        sum += arr[i];
    return (float)sum / n;
}
int main() {
    int a[5], max = 0;
    printf("Enter 5 temperature readings: ");
    for(int i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
        if(a[i] > max) max = a[i];
    }
    printf("Average = %.2f, Highest = %d", average(a, 5), max);
    return 0;
}
