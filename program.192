#include <stdio.h>
float avg2D(int a[3][3]){
    int sum = 0;
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
            sum += a[i][j];
    return (float)sum / 9;
}
int main(){
    int data[3][3];
    printf("Enter readings of 3 sensors (3 each):\n");
    for(int i=0;i<3;i++)
        for(int j=0;j<3;j++)
            scanf("%d",&data[i][j]);
    printf("Average of all readings = %.2f", avg2D(data));
    return 0;
}
