#include <stdio.h>
float avgOddIndex(int a[], int n){
    int sum=0, count=0;
    for(int i=1;i<n;i+=2){ sum+=a[i]; count++; }
    return (float)sum/count;
}
int main(){
    int s[6];
    printf("Enter 6 readings: ");
    for(int i=0;i<6;i++) scanf("%d",&s[i]);
    printf("Average of odd index values = %.2f", avgOddIndex(s,6));
    return 0;
}
