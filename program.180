#include <stdio.h>
void input(int a[], int n) {
    for(int i = 0; i < n; i++)
        scanf("%d", &a[i]);
}
float avg(int a[], int n) {
    int s = 0;
    for(int i = 0; i < n; i++)
        s += a[i];
    return (float)s / n;
}
int main() {
    int s[5];
    printf("Enter 5 sensor readings: ");
    input(s, 5);
    printf("Average = %.2f", avg(s, 5));
    return 0;
}
