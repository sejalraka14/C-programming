#include <stdio.h>
int main(){
    FILE *fp=fopen("sensor_data.txt","r");
    if(fp==NULL){ printf("File not found"); return 0; }
    int val[10], i=0, sum=0;
    while(fscanf(fp,"%d",&val[i])!=EOF){ sum+=val[i]; i++; }
    fclose(fp);
    printf("Average of %d readings = %.2f", i, (float)sum/i);
    return 0;
}
